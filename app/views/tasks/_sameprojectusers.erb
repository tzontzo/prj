    <div class="col-md-4 col-md-offset-4">
      <div class="col-md-12 bg-info ">
        <h4 class="text-center">Users working at <%= @project.name %></h4>
        <table class="table ">
          <thead class="text-center">
          <tr>
            <th ></th>
          </tr>
          </thead>
          <% @project.users.find_each do |u| %>
              <tbody class="text-center">
              <tr>
                <% if u != @user %>
                    <td><%= u.name%></td>
                <%end%>
              </tr>
              </tbody>
          <%end%>
        </table>
      </div>
    </div>
