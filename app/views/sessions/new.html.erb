
<!DOCTYPE html>
<html>
<head>
  <title>Project Management App </title>
  <link href="bootstrap.css" rel="stylesheet">
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  </head>
  <body>
<div class="jumbotron text-center">
<h1 >Log in </h1>
</div  >
<%= form_for(:session, url: login_path) do |f| %>
    <p class="text-center"><%= f.email_field :email, placeholder: "Email" %></p>
    <p class="text-center"><%= f.password_field :password, placeholder: "Password" %></p>
    <p class="text-center"><%= f.submit "Log in"%></p>

<% end %>
</body>
</html>