<h1 style="text-align: center">Create new project</h1>

<%= form_for @project, url: new_project_path(@project) do |f| %>
    <p ><%= f.text_field :name, :placeholder => "Name" %></p>
    <p ><%= f.text_area :description, :placeholder => "Description"%> </p>
    <p ><%= f.submit "Create project" %></p>
<% end %>