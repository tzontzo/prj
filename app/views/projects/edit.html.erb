
<div class="jumbotron text-center" style="background: transparent">
  <h2 >Update project </h2>
</div  >

<div class="text-center">
  <%= form_for @project, url: project_path(@project), method: :patch do |f| %>
      <p ><%= f.text_field :name, placeholder: "Name" %></p>
      <p ><%= f.text_area :description, placeholder: "Description"%> </p>

      <p ><%= f.submit "Update project" %></p>
  <% end %>

</div>
<p class="text-center"><%= link_to 'Back', '/dashboard' %></p>

